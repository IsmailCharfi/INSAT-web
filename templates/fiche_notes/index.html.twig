{% extends 'base.html.twig' %}

{% block body %}
    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for fiche_note in fiche_notes %}
            <tr>
                <td>{{ fiche_note.id }}</td>
                <td>{{ fiche_note.nom }}</td>
                <td>
                    <a href="{{ path('fiche_notes_show', {'id': fiche_note.id}) }}">show</a>
                    <a href="{{ path('fiche_notes_edit', {'id': fiche_note.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}
