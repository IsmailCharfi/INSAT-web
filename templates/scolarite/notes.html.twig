{% extends 'table.html.twig' %}

{% block title %}Notes{% endblock %}

{% block body %}

    <form method="POST" id="my_form">
    <table  class="table" >
        <thead>
        <tr>
            <th>nom</th>
            <th>prenom</th>
            <th>num_inscription</th>
            <th>note</th>


        </tr>
        </thead>
        {% for etud in etudiants %}
            <tr>
        <td>{{ etud.etudiant.nom }}</td>
        <td>{{ etud.etudiant.prenom }}</td>
        <td>{{ etud.etudiant.numInscription }}</td>
        <td><input type="text" id="my_form" name={{etud.etudiant.numInscription}} ></td>
            </tr>
        {% endfor %}


        <tbody>
        </tbody>
        <tfoot>
        <tr>
            <th>nom</th>
            <th>prenom</th>
            <th>num_inscription</th>
            <th>note</th>
        </tr>
        </tfoot>
    </table>
        <button type="submit" class="btn btn-default">Validate</button>
    </form>
{% endblock %}